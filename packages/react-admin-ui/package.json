{
    "name": "@genstackio/react-admin-ui",
    "version": "0.8.7",
    "description": "React Components for Genstackio",
    "license": "MIT",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "directories": {
        "lib": "lib",
        "test": "__tests__"
    },
    "files": [
        "lib",
        "postcss.config.js",
        "tailwind.config.js",
        "assets/css/styles.css",
        "assets/css/themes"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "generate-themes": "themes2tailwindcss ./assets/themes ./assets/css/themes",
        "generate-svg-components": "svg2comp ./assets/svg ./src/images && svg2comp ./assets/svg/icons ./src/images/icons && svg2comp ./assets/svg/icons/flags ./src/images/icons/flags",
        "generate": "yarn --silent generate-svg-components && yarn --silent generate-themes",
        "build": "yarn --silent generate && tsc",
        "test": "../../node_modules/.bin/jest -c ../../jest.config.js --rootDir=`pwd`",
        "story": "yarn --silent generate && start-storybook -c ../../.storybook -p 6006",
        "build-storybook": "yarn --silent generate && build-storybook -c ../../.storybook -o public"
    },
    "dependencies": {
        "@date-io/date-fns": "^2.10.11",
