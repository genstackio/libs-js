{
  "name": "@genstackio/react-admin-ui",
  "version": "0.6.6",
  "description": "React Components for Genstackio",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "postcss.config.js",
    "tailwind.config.js",
    "assets/css/styles.css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "generate-svg-components": "svg2comp ./assets/svg ./src/images && svg2comp ./assets/svg/icons ./src/images/icons",
    "generate": "yarn --silent generate-svg-components",
    "build": "yarn --silent generate && tsc",
    "test": "../../node_modules/.bin/jest -c ../../jest.config.js --rootDir=`pwd`",
    "story": "yarn --silent generate && start-storybook -c ../../.storybook -p 6006",
    "build-storybook": "yarn --silent generate && build-storybook -c ../../.storybook -o public"
  },
  "dependencies": {
    "@material-ui/data-grid": "^4.0.0-alpha.30",
