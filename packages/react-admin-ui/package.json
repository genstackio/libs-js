    "apexcharts": "^3.26.3",
    "i18next": "^20.2.1",
    "react-apexcharts": "^1.3.9",
    "react-color": "^2.19.3",
    "react-dropzone-uploader": "^2.11.0",
    "react-hook-form": "^7.2.3",
    "react-i18next": "^11.8.13",
    "react-player": "^2.9.0",
    "swiper": "^6.5.7",
    "date-fns": "^2.22.1"
